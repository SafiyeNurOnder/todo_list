           <property name="styleSheet">
            <string notr="true">QCalendarWidget {
    border: 2px solid #ccc; /* Takvim çerçevesi rengi */
    border-radius: 5px; /* Köşeleri yuvarlat */
    background-color: #B8E2FF; /* Arka plan rengi */
	font: 12pt;
}
/*
QCalendarWidget QcalenderWidget{
		alternate-background-color: B8E2FF;
}?*/

/* Takvim başlığı */
QCalendarWidget QAbstractItemView {
    background-color: #e0e0e0; /* Başlık arka plan rengi */
    color: #333; /* Başlık metin rengi */
    selection-color: white; /* Seçili tarih metin rengi */
    selection-background-color: #0078d7; /* Seçili tarih arka plan rengi */
}

/* Takvim içeriği */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: #fff; /* İçerik arka plan rengi */
	border: 2px solid #B8E2FF;
	border-bottom: 0px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}

/* Takvim navigasyon butonları */
QCalendarWidget QToolButton {
    background-color: #ffffff; /* Navigasyon butonları arka plan rengi */
    color: black; /* Navigasyon butonları metin rengi */
    border: none; /* Kenarlık yok */
    padding: 5px; /* Buton içeriği boşluğu */
    width: 35px; /* Buton genişliği */
    height: 20px; /* Buton yüksekliği */
}

/* Hover etkisi */
QCalendarWidget QToolButton:hover {
    background-color: #005ea2; /* Hover rengi */
}

/* Seçili tarih */
QCalendarWidget QToolButton:selected {
    background-color: #003f76; /* Seçili buton rengi */
}
/* Önceki ay ve sonraki ay butonları */
QCalendarWidget QToolButton#qt_calendar_prevmonth {
    background-color: transparent; /* Önceki ay butonu arka plan rengi */
    color: white; /* Önceki ay butonu metin rengi */
    border: none; /* Kenarlık yok */
    padding: 5px; /* Buton içeriği boşluğu */
    min-width: 13px; /* Buton genişliği */
	max-width: 13px;
	min-height: 13px;/* Buton yüksekliği */
    max-height: 13px; 
	margin-left: 5px;
}

QCalendarWidget QToolButton#qt_calendar_nextmonth {
    background-color: transparent; /* Sonraki ay butonu arka plan rengi */
    color: white; /* Sonraki ay butonu metin rengi */
    border: none; /* Kenarlık yok */
    padding: 5px; /* Buton içeriği boşluğu */
    min-width: 13px; /* Buton genişliği */
	max-width: 13px;
	min-height: 13px;/* Buton yüksekliği */
    max-height: 13px; 
	margin-right: 5px;
}
/* Ay ve yıl bilgisi */
QCalendarWidget QComboBox {
    min-width: 100px; /* Minimum genişlik */
    min-height: 30px; /* Minimum yükseklik */
    padding: 5px; /* Buton içeriği boşluğu */
    font-size: 13px; /* Yazı tipi boyutu */
}
